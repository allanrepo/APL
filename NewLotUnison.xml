    <Entry Label="DateCode"> </Entry>
    <Entry Label="LotID"> </Entry>
    <Entry Label="TestTemp"> </Entry>
    <Entry Label="HandlerType"> </Entry>
    <Entry Label="ProberHandlerID" Type="Choice">
		<Value> </Value>
		<Value>Simulator</Value>
 	</Entry>

    <Entry Label="TestFacility" ReadOnly="Yes"> </Entry>
